<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jareven.foreverlove">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/basemodel_app_icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/basemodel_app_icon"
        android:supportsRtl="true"
        android:theme="@style/basemodel_AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:targetApi="n">

        <activity
            android:name=".OpenActivity"
            android:exported="true"
            android:theme="@style/basemodel_AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--春节icon-->
        <activity-alias
            android:name="chunjie"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/basemodel_new_icon"
            android:roundIcon="@mipmap/basemodel_new_icon"
            android:screenOrientation="portrait"
            android:targetActivity=".OpenActivity"
            android:theme="@style/basemodel_AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity-alias>


        <!--中秋icon-->
        <activity-alias
            android:name="zhongqiu"
            android:enabled="false"
            android:exported="true"
            android:icon="@mipmap/basemodel_zhongqiu_icon"
            android:roundIcon="@mipmap/basemodel_zhongqiu_icon"
            android:screenOrientation="portrait"
            android:targetActivity=".OpenActivity"
            android:theme="@style/basemodel_AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity-alias>

    </application>

</manifest>